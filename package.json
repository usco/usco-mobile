{
  "name": "shapeFu",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "testOld": "ava src/common/ --require babel-register --watch --verbose",
    "test": "ava './src/common/**/*.test.js' --require babel-register --verbose",
    "build": "browserify src/shapeFu/editor.js -o dist/shapeFu/editor.js -t [ babelify]",
    "build-srv": "babel src/ -d dist",
    "build-proto1": "browserify src/proto1-basics/index.js -o dist/proto1-basics/editor.js -t [babelify browserify minifyify ]",
    "watch": "watchify src/proto1-basics -o dist/index.js -t [ babelify]",
    "start-dev-proto1": "budo src/proto1-basics:dist/bundle.js --port=8080 --live -- -t babelify",
    "start-dev-shapefu": "budo src/shapeFu --port=8080 --live -- -t babelify",
    "test-render": "node src/serverSide/launch.js",
    "test-scadParse": "node src/shapeFu/parsing/launchTestParse.js",
    "test-scadParse2": "jison src/shapeFu/parsing/openscad.jison -m commonjs -o src/shapeFu/parsing/openscadParser.js",
    "scad-editor": "npm run test-scadParse2 && npm run build && http-server -p 8090",
    "host": "http-server -p 8090"
  },
  "author": "Mark 'kaosat-dev' Moissette",
  "license": "MIT",
  "dependencies": {
    "@most/create": "^1.1.3",
    "@most/prelude": "^1.4.1",
    "angle-normals": "^1.0.0",
    "bunny": "^1.0.1",
    "camera-picking-ray": "^1.0.1",
    "concat-stream": "^1.5.2",
    "fast.js": "^0.1.1",
    "fetch-readablestream": "^0.1.0",
    "gl": "^4.0.0",
    "gl-mat4": "^1.1.4",
    "glslify": "^5.0.2",
    "glslify-sync": "^2.0.0",
    "jison": "^0.4.17",
    "most": "^1.0.3",
    "mouse-change": "^1.2.1",
    "mouse-wheel": "^1.1.0",
    "pngjs": "^2.3.1",
    "ray-aabb-intersection": "^1.0.1",
    "ray-sphere-intersection": "^1.0.0",
    "ray-triangle-intersection": "^1.0.3",
    "regl": "^1.1.1",
    "stream-http": "^2.4.0",
    "usco-stl-parser": "github:usco/usco-stl-parser#streaming",
    "vertices-bounding-box": "^1.0.0",
    "web-streams-polyfill": "github:creatorrr/web-streams-polyfill",
    "whatwg-fetch": "^1.0.0",
    "xhr-stream": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.3.0",
    "minifyify": "^7.3.3",
    "rollupify": "^0.3.4",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "glslify-sync/transform",
      "rollupify"
    ]
  }
}
